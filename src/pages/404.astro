---
import MainLayout from "../layouts/MainLayout.astro";
import Image from "astro/components/Image.astro";
import error from '../images/error-404.png'
---

<MainLayout title="404 - Tidak Ditemukan">
    <div class ="flex flex-col items-center justify-items-center gap-7">
        <Image src={error} alt="404" height={250} width={250} class="mt-10" />
        <a href="/" class="inline-block bg-gray-100 rounded-lg border p-2 mb-6 hover:bg-tt-teal hover:text-white">Kembali ke Beranda</a>
    </div>
</MainLayout>